{
	"name": "astro-test",
	"version": "0.0.1",
	"scripts": {
		"dev": "cross-env NODE_OPTIONS=\"--max-http-header-size=65536\" astro dev",
		"preview": "cross-env NODE_OPTIONS=\"--max-http-header-size=65536\" astro preview",
		"astro": "rm -rf .pnpm-store && astro",
		"serve": "nest start --watch --debug  --path tsconfig.server.json",
		"build:serve": "nest build --path tsconfig.server.json",
		"build:client": "astro check && astro build",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:drop": "drizzle-kit drop",
		"db:seed": "ts-node -r tsconfig-paths/register  --project tsconfig.server.json --transpile-only src/infrastructure/providers/database/seeder/run-seed.ts",
		"db:push": "drizzle-kit push",
		"db:push:test": "export NODE_ENV=TEST && drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"biome": "pnpx @biomejs/biome  check  --diagnostic-level=error  --write",
		"test": "vitest run",
		"test:unit": "vitest run",
		"test:watch": "vitest --watch",
		"test:cov": "vitest run --coverage",
		"test:e2e:db": "vitest run --config vitest.e2e.config.ts",
		"prepare": "husky"
	},
